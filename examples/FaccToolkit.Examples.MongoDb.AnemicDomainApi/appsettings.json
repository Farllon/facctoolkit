{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDBConfigurations": {
    "DatabaseName": "blog"
  },
  "ConnectionStrings": {
    "MongoDB": "mongodb://127.0.0.1:27017/?replicaSet=rs0",
    "MongoDBReadOnly": "mongodb://127.0.0.1:27017/?replicaSet=rs0",
    "Redis": "localhost:6379"
  },
  "RedisConfiguration": {
    "Database": 0,
    "Prefix": "my-application",
    "ExpirationInMilliseconds": 30000,
    "SuppressCacheSetErrors":  true
  }
}
